<div class="row">
  <div class="col-12">
    <div class="card rounded-0">
      <div class="card-body">
        <p>
          <strong>Course:</strong>
          <%= @educational_credential.course %>
        </p>
         
        <p>
          <strong>Board or University:</strong>
          <%= @educational_credential.board_or_university %>
        </p>
         
        <p>
          <strong>Name:</strong>
          <%= @educational_credential.city %>
        </p>
         
        <p>
          <strong>Established from:</strong>
          <%= @educational_credential.marks %>
        </p>
         
        <%= link_to 'Edit', edit_admin_educational_credential_path(@educational_credential), class: "btn btn-warning btn-sm" %>  
        <%= link_to 'Back', admin_educational_credentials_path, class: "btn btn-info btn-sm" %>        
      </div>
    </div>
  </div>
</div>

