<div class="row">
  <div class="col-12">
    <%= render 'partials/flash' %>
    <div class="card rounded-0">
      <div class="card-body">        
        <p>
          <strong>Name:</strong>
          <%= @email_notification.subject %>
        </p>
         
        <p>
          <strong>Description:</strong>
          <%= @email_notification.description %>
        </p>

        <%= link_to 'Edit', edit_admin_email_notification_path(@email_notification), class: "btn btn-warning btn-sm" %>  
        <%= link_to 'Back', admin_email_notifications_path, class: "btn btn-info btn-sm" %>
      </div>
    </div>
  </div>
</div>

